{
  "name": "brisket-create-app",
  "version": "1.0.0",
  "description": "Create backend boilerplates with one command",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "node ../../scripts/rm.mjs dist",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "build": "npm run release",
    "prepublishOnly": "npm version patch"
  },
  "keywords": [
    "fastify",
    "app",
    "nodejs",
    "boilerplate",
    "project",
    "cli"
  ],
  "author": "HoaX7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HoaX7/brisket.git",
    "directory": "packages/brisket-create-app"
  },
  "bin": {
    "brisket-create-app": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "package.json",
    "../../README.md"
  ],
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^22.8.7",
    "@types/prompts": "^2.4.9",
    "@types/validate-npm-package-name": "^4.0.2",
    "@vercel/ncc": "^0.38.2",
    "async-retry": "^1.3.3",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "cross-spawn": "^7.0.3",
    "fast-glob": "^3.3.2",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "update-check": "^1.5.4",
    "validate-npm-package-name": "^6.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
